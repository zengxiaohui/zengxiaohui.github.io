{% extends "./inc/backbase.html" %} {% block style %}


{% endblock %} {% block content %}
<div id="content">
  <div id="content-header">
    <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> 账号管理</a> <a href="#" class="current">人事管理</a> </div>

    <h1>人事管理</h1>
  </div>
  <div class="container-fluid">
    <hr>
    <div class="row-fluid">
      <div class="span12">
        
      
       
        
        <div class="widget-box">
          <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
           <h5>Data table</h5>
            <button type="button" class=" btn btn-sm btn-info" id="adduser" onclick="adduser()">添加用户信息</button>
            <!-- <h2>{{userlist}}</h2> -->
          </div>
          <div class="widget-content nopadding">
            <table class="table table-bordered data-table with-check" id="mytable" >

            <thead>
                <tr>
                  <th style="width:15%">编号</th>
                  <th>用户名</th>
                  <th>用户身份</th>
                  <th>真实姓名</th>
                  <th>用户电话</th>
                  <th>用户邮箱</th>
                  <th data-formatter="actionformatter"
            data-events="actionevents">操作</th>
                </tr>
              </thead>
              <tbody>
               
              
            {% block header %} 
                {% for elm1 in userlist %}

  <tr>
        <td style="width:15%">{{elm1.userId}}</td>
        <td>{{elm1.name}}</td>
          <!-- <td>{{userRoles}}</td>  -->
        {% if elm1.userRoles == 'admin'%}
        <td>管理员
        {% elif elm1.userRoles== 'doctor'%}
        <td>医生
        {% elif elm1.userRoles== 'sales'%}
        <td>销售
        {% elif elm1.userRoles== 'patient'%}
        <td>病人
        {% elif elm1.userRoles== 'tester'%}
        <td>医学部用户
        {%endif%} </td>
        <td>{{elm1.realName}}</td>
        <td>{{elm1.phone}}</td>
        <td>{{elm1.email}}</td>
        <td><a href="/back/userinfor?userId={{elm1.userId}}&name={{elm1.name}}&userRoles={{elm1.userRoles}}&phone={{elm1.phone}}&email={{elm1.email}}&password={{elm1.password}}&realName={{elm1.realName}}" >查看</a>|<a 
        onclick="deleteUser('{{elm1.userId}}')">删除</a></td>
    {% endfor %}
    {% endblock %}
  </tr>
           </tbody>   
            </table>
           <!--   -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!--Footer-part-->
<!--this is tip information-->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          &times;
        </button>
        <h4 class="modal-title" id="myModalLabel" >
          添加用户信息

        </h4>
        <span style="font-size: 15px;color: red"  id="pwd" hidden>两次密码输入不一样</span>
      </div>
      <div class="modal-body">
        <form class="form-horizontal" role="form" style="margin: 0 auto" id="createUser" action="/backextra/adduser" method="post">
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="text" class="form-control" name = "name" id="firstname" 
           placeholder="请输入用户名" required oninvalid="setCustomValidity('请输入用户名！');" oninput="setCustomValidity('');">
    </div>
  </div>
  <br>
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="password" class="form-control" id="password" 
           placeholder="请输入密码" required  name="password">
    </div>
  </div>
  <br>
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="password" class="form-control" id="password1" 
           placeholder="请确认密码">
    </div>
  </div>
  <br>
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="text" class="form-control" id="phone" 
           placeholder="请输入电话号码" name="phone">
    </div>
  </div>
  <br>
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="text" class="form-control" id="realName" 
           placeholder="请输入真实姓名" name="realName">
    </div>
  </div>
  <br>
  <div class="form-group">
    
    <div class="col-sm-10">
      <input type="email" class="form-control" id="email" 
           placeholder="请输入邮箱" name="email">
    </div>
  </div>
  <br>

  <div class="form-group">
    <div class="col-sm-10">
      
     <select name="userRole" class="" id="selectRoles"  name="role" >
        <option value="admin">管理员</option>
        <option value="doctor">医生</option>
        <option value="patient" selected="selected">患者</option>
        <option value="sales">销售</option>
        <option value="tester">医学部用户</option>
      </select>
    </div>
  </div>
 
</form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">关闭
        </button>
        <button type="submit" class="btn btn-primary" id="deleteUser" form="createUser">
          添加用户
        </button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal -->
</div>


<script  type="text/javascript" charset="utf-8" >


  function adduser(){

    window.location.href="/back/adduser";
  }

function deleteUser(a){
      if (!confirm("确认要删除？")) {
          window.event.returnValue = false;
        }else{
          window.location.href="/back/deleteuser?userId="+a+"" 
        }

}
</script>
{% endblock %} {% block script %}





{% endblock %}