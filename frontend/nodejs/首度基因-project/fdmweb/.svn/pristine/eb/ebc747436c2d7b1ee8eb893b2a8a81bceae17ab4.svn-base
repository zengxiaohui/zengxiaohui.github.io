  <script type="text/javascript" src="/static/assets/js/bootstrap-datetimepicker.min.js"></script>
 <script  type="text/javascript" charset="utf-8" async defer>


  


  var username = $.cookie("name");

  //alert(username);
 $("#user").text(username);



$('.form_date').datetimepicker({
        language:  'fr',
        weekStart: 1,
        todayBtn:  1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		minView: 2,
		forceParse: 0,
    format: 'yyyy-mm-dd'
    });


$("#submitdisc").click(function(){

    var endDate = $("#endDate").val();
var startDate = $("#startDate").val();


var startDate1 = startDate.split("-");
var endDate1 = endDate.split("-");
var startDate2 = startDate1[0]*365*30*3600+startDate1[1]*30*3600+startDate1[2]*3600;
var endDate2 = endDate1[0]*365*30*3600+endDate1[1]*30*3600+endDate1[2]*3600;


if(endDate2<=startDate2){
  $("#timetip").css("display","block") ;
}else{
  $("#createUser").submit();
}
   
  

})
            </script>
