    <h1>产品管理</h1>
  </div>
  <div class="container-fluid">
    <hr>
    	<form class="form-horizontal " role="form" action="/backextra/adddiscprice" id="createUser" style="text-align: center;" method="post">
  <div class="form-group">
    
    <div class="col-sm-10 form-inline">
    <label>产品名称：</label>
      <select name="itemid" >
      {% for elm1 in itemList %}
  
  <option value="{{elm1.itemId}}">{{elm1.name}}</option>

{% endfor %}
      	
      </select>
    </div>
  </div>
  <br>
  
  <div class="form-group form-inline">
    
    <div class="col-sm-10">
  <label>开始时间：</label> 
     
       <input size="16" type="text" value="{{discinfor.startDate}}"  id="startDate" name="startDate" class="form-control input-append date form_datetime form_date" placeholder="请输入开始日期"  data-link-field="dtp_input1" required="">
       <!-- class="controls input-append date form_date" data-date="" data-date-format="dd MM yyyy" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd"  -->
                   
                    
                
    </div>
  </div>
  <br>
  <div class="form-group form-inline">
    
    <div class="col-sm-10">
    <label>结束时间：</label>
       <input size="16" type="text" value="{{discinfor.startDate}}" id="endDate" name="endDate" class="form-control input-append date form_datetime form_date" placeholder="请输入结束日期" data-link-format="yyyy-mm-dd" data-link-field="dtp_input1" required="" >
    </div>
    <span style="color:red ;font-size: 12px ;display: none" id="timetip">开始时间必须小于结束时间</span>
  </div>
  <br>
  <div class="form-group form-inline">
    
    <div class="col-sm-10">
    <label>   折扣码 ：</label>
      <input type="text" class="form-control"  
           placeholder="请输入折扣码"  name="promotionCode" value="AAAAAAAAA"  required="">

          
    </div>

  </div>
  <br>
  <div class="form-group form-inline">
    
    <div class="col-sm-10">
    <label>产品价格：</label>
      <input type="text" class="form-control" id="phone" 
           placeholder="请输入产品价格" value="{{discinfor.price}}" name="price" required="" >
    </div>
  </div>
  <br>
  
 <button type="button" class="btn btn-info" onclick="javascript:void(window.history.back())">返回</button>
 <button  class="btn btn-danger" type="button" id="submitdisc">添加</button>
</form>
