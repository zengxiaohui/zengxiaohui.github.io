<div id="content">
		
		
		<div id="content-body"  style="height:600px; width: 80% ; margin-left: 10%; margin-right: 10% ">
				<br>
				
			<h3 style="text-align: center; color: red">订单详细信息</h3>
			<div id="reportTableDiv" class="span10">
				<br>
				
				<table id="reportTable" >
					<tr class="danger">
						<td class="pill">产品名称</td>
						<td class="pill">数量</td>
					</tr>
					<tr >
						<td>{{itemName1}}</td>
						<td>{{itemName1Num}}</td>
					</tr>
					<tr>
						<td>
						{% if itemName2 ==undefined%}
						------</td>
						
						{%else%}
						{{itemName2}}</td>
						{%endif%}
						<td>
						{% if itemName2Num ==undefined %}
						------</td>
						{%else%}
						{{itemName2Num}}
						</td>
						{%endif%}
					</tr>
					<tr>
						<td>
						{% if itemName3 ==undefined%}
						------</td>
						
						{%else%}
						{{itemName3}}</td>
						{%endif%}
						<td>
						{% if itemName3Num ==undefined %}
						------</td>
						{%else%}
						{{itemName3Num}}
						</td>
						{%endif%}
					</tr>
					
					<tr class="danger">
						<td class="pill">用户名</td>
						<td class="pill">订单号</td>
					</tr>
					<tr>
						<td>{{inforbill.senderName}}</td>
						<td>{{inforbill.orderId}}</td>
					</tr>
					<tr class="danger">
						<td class="pill">优惠码</td>
						<td class="pill">订单生成日期</td>
					</tr>
					<tr>
						<td></td>
						<td>{{sendDate1}}</td>
					</tr>
					<tr class="danger">
						<td class="pill">跟单状态</td>
						<td class="pill">付费状态</td>
					</tr>
					<tr>
					
						{% if  inforbill.testStatus ==6%}
						<td>等待工作人员联系
						{% elif inforbill.testStatus == 5%}
						<td>最终报告已经出具
						{% elif inforbill.testStatus == 4%}
						<td>自动报告已经生成
						{% elif inforbill.testStatus == 3%}
						<td>完成检测
						{% elif inforbill.testStatus == 2%}
						<td>正在处理
						{% elif inforbill.testStatus == 1%}

						<td>收到样本
						</td>
						{%endif%}
						
						{% if inforbill.feeStatus == 1%}
						<td>未缴费
						{%  elif inforbill.feeStatus == 2%}
						<td>部分缴费
						{% elif inforbill.feeStatus == 3%}
						<td>已缴费
						{%endif%}


						


						</td>
					</tr>
					<tr>
						<td ><a href="/report/download">发票下载</a></td>
						<td><span color="red">总价格:</span></td>
					</tr>
				</table>

			</div>
		</div>
	</div>
	

	



