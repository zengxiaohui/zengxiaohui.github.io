{% extends "./inc/backbase.html" %} {% block style %}


{% endblock %} {% block content %}
<div id="content">
  <div id="content-header">
    <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="icon-home"></i>产品管理</a> <a href="#" class="current">折扣列表</a> </div>

    <h1>产品管理</h1>
  </div>


  
  <div class="container-fluid">
    <hr>
    <div class="row-fluid">
      <div class="span12">
        
      
       
        
        <div class="widget-box">
          <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
            <h5>折扣列表</h5>
            <button type="button" class=" btn btn-sm btn-info" id="adddisc" onclick="adddisc()">添加折扣产品</button>
          </div>
          <div class="widget-content nopadding" style=" font-size: 15px; ">
            <table class="table table-bordered data-table with-check" id="mytable" >

            <thead>
                <tr style=" font-size: 15px; ">
                  <th style="width:15%">产品编号</th>
                  <th>产品名称</th>
                  <th>产品折扣码</th>
                  <th>开始时间</th>
                  <th>结束时间</th>
                  <th>产品价格</th>
                  <th>操作</th>
                </tr>
              </thead>
              <tbody>
               
              
            {% block header %} 
                {% for elm1 in disclist %}

  <tr>
        <td style="width:15%">{{elm1.id}}</td>
        {% if elm1.itemId == 1%}
        <td>FD-Tissue-180
        {% elif elm1.itemId== 2%}
        <td>FD-ctDNA-180
        {% elif elm1.itemId== 3%}
        <td>FD-Tissue-600
        {% elif elm1.itemId== 4%}
        <td>FD-ctDNA-600
        {% elif elm1.itemId== 5%}
        <td>FD-Exome
        {% elif elm1.itemId== 6%}
        <td>FD-ctDNA-Exome
        {% elif elm1.itemId== 7%}
        <td>FD-Tissue-RNA
        {% elif elm1.itemId== 8%}
        <td>FD-Tissue-180+FD-ctDNA-180
        {% elif elm1.itemId== 9%}
        <td>FD-Tissue-600+FD-ctDNA-600
        {%endif%}
        </td>
        <td>{{elm1.promotionCode}}</td>
        <td>{{elm1.startDate}}</td>
        <td>{{elm1.endDate}}</td>
        <td>{{elm1.price}}</td>
        <td><a href="/back/discpriceinfor?id={{elm1.id}}&itemId={{elm1.itemId}}&promotionCode={{elm1.promotionCode}}&startDate={{elm1.startDate}} &endDate={{elm1.endDate}}&price={{elm1.price}}">查看</a>|<a onclick="deletePrice('{{elm1.id}}')">删除</a></td>
    {% endfor %}
    {% endblock %}
  </tr>
           </tbody>   
            </table>
            
          </div>
        </div>
      </div>
    </div>
  </div>



</div>
<!--Footer-part-->
<script type="text/javascript" charset="utf-8" async defer>
  

  function adddisc(){

    window.location.href="/backextra/adddiscprice";
  }

 
  function deletePrice(a){

    //alert("test");

    if (!confirm("确认要删除？")) {
          window.event.returnValue = false;
        }else{
          window.location.href="/backextra/deletedisc?id="+a+"" 
        }
    
  }

/*function deleteUser(a){
      if (!confirm("确认要删除？")) {
          window.event.returnValue = false;
        }else{
          window.location.href="/back/deletuser?userId="+a+"" 
        }

}*/
</script>


{% endblock %} {% block script %}




{% endblock %}